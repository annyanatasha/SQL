SELECT SUM(item_id)
FROM superstore;

SELECT MAX(item_id)
FROM superstore;

SELECT COUNT(item_name)
FROM superstore;

SELECT SUM(price),category
FROM superstore
GROUP BY category;

SELECT*
FROM superstore 
ORDER BY price;

SELECT SUM(price)
FROM superstore; 

SELECT item_name, price
FROM superstore 
WHERE category = 'Kitchen Supplies';


SELECT item_name, price
FROM superstore 
WHERE category = 'Electronics';
